{
  "name": "happyr/json-api-response-factory",
  "description": "Response factory in compliance with apijson",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Radoje Albijanic",
      "email": "radoje.albijanic@gmail.com"
    },
    {
      "name": "Tobias Nyholm",
      "email": "tobias.nyholm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "league/fractal": "^0.18 || ^0.19 || ^0.20",
    "symfony/http-foundation": "^5.4 || ^6.0 || ^7.0",
    "symfony/validator": "^5.4 || ^6.0 || ^7.0"
  },
  "require-dev": {
    "nyholm/nsa": "^1.1",
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Happyr\\JsonApiResponseFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Happyr\\JsonApiResponseFactory\\": "tests/"
    }
  }
}
