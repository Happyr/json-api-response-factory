{
  "name": "happyr/json-api-response-factory",
  "type": "library",
  "description": "Response factory in compliance with apijson",
  "license": "MIT",
  "authors": [
    {
      "name": "Radoje Albijanic",
      "email": "radoje.albijanic@gmail.com"
    },
    {
      "name": "Tobias Nyholm",
      "email": "tobias.nyholm@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "league/fractal": "^0.18 || ^0.19",
    "symfony/http-foundation": "^4.4 || ^5.2 || ^6.0",
    "symfony/validator": "^4.4 || ^5.2 || ^6.0"
  },
  "require-dev": {
    "nyholm/nsa": "^1.1",
    "phpunit/phpunit": "^8.5 || ^9.5"
  },
  "autoload": {
    "psr-4": {
      "Happyr\\JsonApiResponseFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Happyr\\JsonApiResponseFactory\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
